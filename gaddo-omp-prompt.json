{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "butter": "#ffd659",
    "peachy": "#ff855b",
    "plum": "#9A348E",
    "purplecherry": "#b45d89",
    "sky": "#86BBD8",
    "tan": "#CC3802",
    "teal": "#047E84",
    "white": "#FFFFFF"
  },
  "blocks": [
    {
      "type": "rprompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "template": "{{ if .Env.PNPPSHOST }} \uf8c5 {{ .Env.PNPPSHOST }} {{ end }}",
          "foreground": "p:white",
          "background": "p:tan",
          "type": "text",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{ if .Env.PNPPSSITE }} \uf2dd {{ .Env.PNPPSSITE }}{{ end }}",
          "foreground": "p:white",
          "powerline_symbol": "\ue0b0",
          "background": "p:teal",
          "type": "text",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "trailing_diamond": "\ue0b4",
          "template": "{{ if .Env.PNPPSSITE }}Â {{ end }}",
          "foreground": "p:white",
          "background": "p:teal",
          "type": "text",
          "style": "diamond"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "trailing_diamond": "\ue0b0",
          "template": " \uedd9 ",
          "foreground": "#000",
          "background": "p:peachy",
          "type": "text",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " {{ .UserName }} ",
          "foreground": "#000",
          "background": "p:butter",
          "type": "session",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "max_depth": 3,
            "style": "agnoster_short"
          },
          "template": " {{ .Path }} ",
          "foreground": "#000",
          "powerline_symbol": "\ue0b0",
          "background": "p:white",
          "type": "path",
          "style": "powerline"
        },
        {
          "properties": {
            "branch_icon": "",
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "template": " {{ .UpstreamIcon }}[\uf126 {{ .HEAD }}] {{.BranchStatus}} {{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }}",
          "foreground": "p:white",
          "powerline_symbol": "\ue0b0",
          "background": "p:purplecherry",
          "type": "git",
          "style": "powerline"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "overflow": "break",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "style": "dallas",
            "threshold": 0
          },
          "template": " \ueba2 {{ .FormattedMs }}s ",
          "foreground": "#ffffff",
          "background": "#333333",
          "type": "executiontime",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "time_format": "15:04:05"
          },
          "template": "<transparent> \ue0ba\ue0bc </>{{ .CurrentDate | date .Format }} ",
          "foreground": "#ffffff",
          "background": "#333333",
          "type": "time",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "time_format": "Monday"
          },
          "template": "<#cc7eda>|</> {{ .CurrentDate | date .Format }}",
          "foreground": "#fff",
          "background": "#333333",
          "type": "time",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "display_mode": "always",
            "fetch_version": true
          },
          "template": "<#cc7eda> | </><#7eb8da>\ue718</> {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "foreground": "#3EC669",
          "background": "#333333",
          "type": "node",
          "style": "powerline"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\ue3bf ",
          "foreground": "#cd5e42",
          "type": "root",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": ">>",
          "foreground": "#f5af78",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
